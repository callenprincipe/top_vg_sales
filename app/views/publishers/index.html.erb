<h1 class="title is-1">Publishers</h1>

<div class="columns is-multiline" id="genres">
  <% @publishers.each do |publisher| %>
    <div class="column is-one-third">
      <div class="box">
        <p class="subtitle is-4">
          <%= link_to publisher.name, publisher, class: "has-text-info" %>
        </p>
      </div>
    </div>
  <% end %>
</div>